{
  "name": "ac-notification-bot-ts",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/odanado/ac-notification-bot-ts",
  "author": "odan",
  "license": "MIT",
  "scripts": {
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "build": "yarn run lint && webpack --mode=production",
    "dev": "yarn run lint && webpack --mode=development",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/google-apps-script": "^0.0.22",
    "@types/jest": "^22.2.3",
    "gas-webpack-plugin": "^0.3.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "inversify": "^4.13.0",
    "reflect-metadata": "^0.1.12",
    "typed-slack": "^0.1.3"
  }
}
