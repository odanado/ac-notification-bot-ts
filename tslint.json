{
  "extends": [
    "tslint-config-standard",
    "tslint-consistent-codestyle"
  ],
  "rules": {
    "naming-convention": [
      true,
      {
        "type": "member",
        "format": ["camelCase", "UPPER_CASE"],
        "leadingUnderscore": "forbid",
        "trailingUnderscore": "forbid"
      },
      {
        "type": "interface",
        "prefix": "I"
      },
      {
        "type": "property",
        "modifiers": [
          "static",
          "const"
        ],
        "format": "UPPER_CASE"
      }
    ],
    "semicolon": true
  }
}
